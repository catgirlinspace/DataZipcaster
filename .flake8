[flake8]
max-line-length = 80
extend-ignore = E203
per-file-ignores = 
    __init__.py:F401, E402
    data_zipcaster/base_plugins.py:DAR202
    data_zipcaster/importers/example_importer.py:T201
    tests\*:DAR101, DAR201, F401
exclude = 
    test.py
    .venv
    docs
    data_zipcaster/cli
[darglint]
docstring_style = google
strictness = full
indentation = 4